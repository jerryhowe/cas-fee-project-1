{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "node": true,
    "es2021": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "indent": ["error", 4],
    "no-console": "off",
    "no-plusplus": "off",
    "import/prefer-default-export": "off"
  }
}